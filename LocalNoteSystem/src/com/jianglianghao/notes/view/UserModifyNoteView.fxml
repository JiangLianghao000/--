<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jianglianghao.notes.view.UserModifyNoteViewController">
   <children>
      <HTMLEditor fx:id="modifyNoteContent" htmlText="&lt;html&gt;&lt;head&gt;&lt;/head&gt;&lt;body contenteditable=&quot;true&quot;&gt;&lt;/body&gt;&lt;/html&gt;" layoutX="2.0" prefHeight="354.0" prefWidth="1000.0" />
      <Button fx:id="finishModify" layoutX="802.0" layoutY="621.0" mnemonicParsing="false" onMouseClicked="#finishModify" prefHeight="59.0" prefWidth="168.0" text="修改完成" />
      <TextField fx:id="modifyTitle" layoutX="99.0" layoutY="626.0" prefHeight="40.0" prefWidth="192.0" promptText="标题" />
      <Label layoutX="27.0" layoutY="626.0" prefHeight="48.0" prefWidth="81.0" text="标题" />
      <RadioButton fx:id="choicePublic" layoutX="318.0" layoutY="629.0" mnemonicParsing="false" onMouseClicked="#choicePublic" text="公开">
         <font>
            <Font size="32.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="privateOrPublic" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="choicePrivate" layoutX="464.0" layoutY="626.0" mnemonicParsing="false" onMouseClicked="#choicePrivate" prefHeight="48.0" prefWidth="200.0" text="私有" toggleGroup="$privateOrPublic">
         <font>
            <Font size="33.0" />
         </font>
      </RadioButton>
      <Button fx:id="showInEditor" layoutX="716.0" layoutY="386.0" mnemonicParsing="false" onMouseClicked="#showInEditor" prefHeight="48.0" prefWidth="156.0" text="预览" />
      <Button fx:id="showInText" layoutX="43.0" layoutY="386.0" mnemonicParsing="false" onMouseClicked="#showInText" prefHeight="48.0" prefWidth="168.0" text="展示文本" />
      <TextArea fx:id="noteText" layoutX="23.0" layoutY="446.0" prefHeight="150.0" prefWidth="959.0" />
   </children>
</AnchorPane>
